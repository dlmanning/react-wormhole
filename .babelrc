{
  "optional": [
    "es7.objectRestSpread",
    "es7.asyncFunctions",
    "runtime"
  ],
  "blacklist": [
    "es6.forOf",
    "es6.blockScoping",
    "es6.arrowFunctions"
  ],
  "plugins" : [
    "react-transform"
  ],
  "extra"  : {
    "react-transform": {
      "transforms": [{
        "transform": "react-transform-hmr",
        "imports": ["react"],
        "locals": ["module"]
      }]
    }
  },
  "ignore": [
    "node_modules"
  ]
}
